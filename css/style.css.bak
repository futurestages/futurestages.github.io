*, ::after, ::before {
    box-sizing: border-box;
}
html {
  scroll-behavior: smooth;
}
body{
	margin:0px;
	background-image: url(../img/bg.webp);
	background-repeat: no-repeat;
	background-size: cover;
	background-attachment: fixed;
	background-color: #ffe4ff;
	font-family: "Roboto", sans-serif;
}
header{
    padding: 13px 22px;
	font-family: "Oswald", sans-serif;
	font-weight:400
}
.headerwithlogo {
    position: fixed;
    width: 100%;
    top: -100px;
	transition: top 0.3s, background-color 0.3s; 
}
.header-right{
	align-items: center;
    display: flex;
}	
.visible {
	top: 0;
	background-color: #9e009d; 
	z-index:99999;
	padding: 0px 13px;
	
}
.visible .header-right a{
	background-color:white;
	color: #9e009d;
}
.headerwithlogo img{
	width: 133px;
}
.container{
	width:1250px;
	padding:0px 20px;
	max-width:100%;
	margin:0px auto;
	overflow:hidden;
	position:relative;
}
.header-flex{
	display: flex;
    gap: 22px;
    justify-content: space-between;
}
.header-left{
	display: flex;
    align-items: center;
    gap: 12px;
    font-size: 15px;
    color:#9e009d;
}
.header-right a{
	padding:12px 31px;
    display: inline-block;
    background-color: #9e009d;
    color: white;
    text-decoration: none;
	font-size: 21px;
}
.header-left div{
	display: flex;
    align-items: center;
    gap: 11px;
    font-size: 21px;
	padding: 10px 16px;
	background-color: white;
	cursor:pointer;
}
.header-left div i{
	font-size:22px;
}

.swiper-button-prev, .swiper-button-next {
	color: #9e009d;
	width: 30px; /* Adjust the width as needed */
	height: 30px; /* Adjust the height as needed */
}
.swiper-button-prev::after, .swiper-button-next::after {
	font-family: "FontAwesome";
	font-weight: 900;
	font-size: 20px;
}
.swiper-button-prev::after {
	content: "\f053"; /* Font Awesome left arrow */
}
.swiper-button-next::after {
	content: "\f054"; /* Font Awesome right arrow */
}
.quote{
