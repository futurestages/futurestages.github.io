.customTable {
    width: 100%;
    border-collapse: collapse;
    text-align: left;
    font-size: 18px;
}
table > thead > tr > th {
    color: black;
    font-size: 26px;
    text-transform: uppercase;
}
table.customTable td, table.customTable th {
    border-width: 1px;
    border-color: #D6D6D6;
    border-bottom: 1px solid black;
    padding: 20px;
}
table > tbody > tr > td:nth-child(4) {
    text-align: center;
    background-color: black;
    font-weight: bold;
}

table.customTable td, table.customTable th {
    border-width: 1px;
    border-color: #D6D6D6;
    border-bottom: 1px solid black;
    padding: 20px;
}
table a{
	color:white;
}
table a:hover{
	border-bottom:1px solid white;
}	
@media screen and (max-width: 800px) {
    table tr {
		border: none !important;
        display: grid;
    }
	th{
		display:none;
    }
	table.customTable td, table.customTable th{
		text-align:center;
		border:0px !important;
    }
	table td:nth-child(1) {
        font-weight: bold;
        font-size: 22px;
        padding: 0.5em 0.75em 0 0.50em;
        margin-top: 20px;
    }
	
}
@media screen and (max-width: 992px) {
	.quote{
		font-size:22px;
	}
	.quote br{
		display:none;
	}
	.banner-logo {
		margin-top: 0;
	}
	.said-by{
		font-size:14px;
	}
	.team-members{
		grid-template-columns: repeat(1, 1fr);
		margin-top: 0px;
	}
	.three-col {
		grid-template-columns: repeat(1, 1fr);
		gap:40px;
	}
	.footer-flex {
		grid-template-columns: repeat(1, 1fr);
		gap:20px;
	}
	.top-sec{
		margin-top: 50px;
	}
	.main-slider .swiper-slide img{
		height:350px;
	}
	.social{
		justify-content:center;
	}
	.text-right {
		text-align: center;
	}
	.hidemob{
		display:none;
	}
	.header-right a {
		padding: 12px 17px;
	}
	.inner-pop {
		display: grid;
	}
	.showpop{
		overflow:auto;
	}
	.inner-pop{
		overflow: auto;
		max-height: 92vh;
	}
	.two-col{
		grid-template-columns: repeat(1, 1fr);
		gap:20px;
	}
	.htwo{
		margin-top:0px;
	}
	
}