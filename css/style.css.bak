.accordion {
	background-color: #ffd3ff;
	cursor: pointer;
    padding: 18px;
    width: 100%;
    border: none;
    text-align: left;
    outline: none;
    font-size: 21px;
    transition: 0.4s;
    font-family: 'Oswald';
    font-weight: 300;
    text-transform: uppercase;
	border-bottom: 1px solid #9e009d;
}


.accordion:after {
  content: '\002B';
  color: #777;
  font-weight: bold;
  float: right;
  margin-left: 5px;
}

.activet:after {
  content: "\2212";
}

